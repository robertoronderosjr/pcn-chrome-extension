{
    "manifest_version": 2,
    "name": "PCN Pro Clubs Stats Automator",
    "description": "This extension will automatically input the stats generated by Fifa Image",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "web_accessible_resources": [
        "team_stats.json"
    ]
}